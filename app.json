{
  "expo": {
    "name": "where-can-i-watch",
    "slug": "where-can-i-watch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/where-icon.png",
    "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#3E4E56"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "privacy": "unlisted"
  }
}
